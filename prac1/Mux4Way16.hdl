CHIP Mux4Way16 {
    IN a[16], b[16], c[16], d[16], sel[2];
    OUT out[16];

    PARTS:
    // First stage: choose between a and b
    Mux16(a=a, b=b, sel=sel[0], out=ab);

    // First stage: choose between c and d
    Mux16(a=c, b=d, sel=sel[0], out=cd);

    // Second stage: choose between the results of the first stage
    Mux16(a=ab, b=cd, sel=sel[1], out=out);
}
